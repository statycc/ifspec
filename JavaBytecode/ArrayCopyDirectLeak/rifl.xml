<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE riflspec SYSTEM "../rifl-JavaBytecode-1.0.dtd">
<riflspec>
  <interfacespec>
    <assignable handle="h">
      <category name="someCat">
        <source>
<parameter class="LEg4;" method="f(II[I)I" parameter="1"/>
</source>
          <source>
<parameter class="LEg4;" method="f(II[I)I" parameter="3"/>
</source>
      </category>
    </assignable>

    <assignable handle="l">
      <category name="anotherCat">
       <sink>
<returnvalue class="LEg4;" method="f(II[I)I"/>
</sink>
      </category>
    </assignable>

  </interfacespec>

  <domains>
<domain name="high"/>
<domain name="low"/>
</domains>

  <flowrelation>
<flow from="low" to="high"/>
</flowrelation>

  <domainassignment>
    <assign domain="high" handle="h"/>
    <assign domain="low" handle="l"/>
  </domainassignment>
</riflspec>

