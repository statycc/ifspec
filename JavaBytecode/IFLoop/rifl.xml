<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE riflspec SYSTEM "../rifl-JavaBytecode-1.0.dtd">
<riflspec>
  <interfacespec>
    <assignable handle="l">
      <category name="lowVertices">
        <sink>
<returnvalue class="LIFLoop;" method="secure_ifl(I)I"/>
</sink>
      </category>
    </assignable>
    <assignable handle="h">
      <category name="highVertices">
        <source>
<parameter class="LIFLoop;" method="secure_ifl(I)I" parameter="1"/>
</source>
      </category>
    </assignable>
  </interfacespec>
  <domains>
<domain name="high"/>
<domain name="low"/>
</domains>
  <flowrelation>
<flow from="low" to="high"/>
</flowrelation>
  <domainassignment>
    <assign domain="high" handle="h"/>
    <assign domain="low" handle="l"/>
  </domainassignment>
</riflspec>

