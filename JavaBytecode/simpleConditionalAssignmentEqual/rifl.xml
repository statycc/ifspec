        <assignable handle="h">
            <source>
<field class="LsimpleConditionalAssignmentEqual;" name="secret"/>
</source>
        </assignable>
        <assignable handle="l">
            <sink>
<returnvalue class="LsimpleConditionalAssignmentEqual;" method="test()I"/>
