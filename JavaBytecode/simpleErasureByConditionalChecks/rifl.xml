<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE riflspec SYSTEM "../rifl-JavaBytecode-1.0.dtd">
<riflspec>
    <interfacespec>
        <assignable handle="h">
                <source>
                    <parameter class="LSimpleErasureByConditionalChecks;" method="computeSecretly(I)I" parameter="1"/>
                </source>
        </assignable>
        <assignable handle="l">
                <sink>
                    <returnvalue class="LSimpleErasureByConditionalChecks;" method="computeSecretly(I)I"/>
                </sink>
        </assignable>
    </interfacespec>
        
    <domains>
<domain name="high"/>
<domain name="low"/>
</domains>
    <flowrelation>
<flow from="low" to="high"/>
</flowrelation>
    <domainassignment> 
         <assign domain="high" handle="h"/>
         <assign domain="low" handle="l"/>
    </domainassignment>
</riflspec>

