<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE riflspec SYSTEM "../rifl-DalvikBytecode-1.0.dtd">
<riflspec>
    <interfacespec>
        <assignable handle="h">
            <category name="highCat">
                <source>
<returnvalue class="Ljava/util/Scanner;" method="nextLine()Ljava/lang/String;"/>
</source>
            </category>
        </assignable>
        <assignable handle="l">
            <category name="lowCat">
                <sink>
<parameter class="Ljava/io/PrintStream;" method="println(I)V" parameter="1"/>
</sink>
            </category>
        </assignable>
    </interfacespec>
    
    <domains>
<domain name="high"/>
<domain name="low"/>
</domains>
    
    <flowrelation>
<flow from="low" to="high"/>
</flowrelation>

    <domainassignment>
        <assign domain="high" handle="h"/>
        <assign domain="low" handle="l"/>
    </domainassignment>

</riflspec>

