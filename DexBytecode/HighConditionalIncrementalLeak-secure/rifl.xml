<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE riflspec SYSTEM "../rifl-DalvikBytecode-1.0.dtd">
<riflspec>
  
  <interfacespec>
    <assignable handle="h">
        <source>
					<parameter class="LEg2;" method="f(II)I" parameter="1"/>
			</source> 
    </assignable>

    <assignable handle="lsrc">
        <source>
					<parameter class="LEg2;" method="f(II)I" parameter="2"/>
				</source> 
		</assignable>
		<assignable handle="lsnk">
        <sink>
					<returnvalue class="LEg2;" method="f(II)I"/>
				</sink> 
    </assignable>

  </interfacespec>

  <domains>
<domain name="high"/>
<domain name="low"/>
</domains>

  <flowrelation>
<flow from="low" to="high"/>
</flowrelation>

  <domainassignment>
    <assign domain="high" handle="h"/>
    <assign domain="low" handle="lsrc"/>
		<assign domain="low" handle="lsnk"/>
  </domainassignment>
</riflspec>

