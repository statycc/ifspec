<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE riflspec SYSTEM "../rifl-DalvikBytecode-1.0.dtd">
<riflspec>
  <interfacespec>
    <assignable handle="h">
      <category name="higSources">
        <source>
<parameter class="Lprogram;" method="foo(Z)Z" parameter="1"/>
</source>
      </category>
    </assignable>
    <assignable handle="l">
      <category name="lowSinks">
        <sink>
<returnvalue class="Lprogram;" method="foo(Z)Z"/>
</sink>
      </category>
    </assignable>
  </interfacespec>

  <domains>
<domain name="high"/>
<domain name="low"/>
</domains>
  <flowrelation>
<flow from="low" to="high"/>
</flowrelation>
  <domainassignment>
    <assign domain="high" handle="h"/>
    <assign domain="low" handle="l"/>
  </domainassignment>
</riflspec>

